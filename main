statement = input("Enter a statement you want to wordify ")
newstatement = ""
newchar = list(statement)
for char in newchar:
    if char == "(" or char == ")" or char == "1" or char == "2" or char == "3" or char == "4" or char == "5" or char == "6" or char == "7" or char == "8" or char == "9" or char == "0":
        char = ''
        newstatement += char
    else:
        newstatement += char
print(newstatement)
